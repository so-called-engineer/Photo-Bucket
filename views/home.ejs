<!DOCTYPE html>
<html lang="en">
<head>
	<title>Photo Bucket</title>
	<meta charset="UTF-8">
	<meta name="description" content="Riddle - Portfolio Template">
	<meta name="keywords" content="portfolio, riddle, onepage, creative, html">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<!-- Favicon -->   
	<link href="img1/favicon.ico" rel="shortcut icon"/>

	<!-- Google Fonts -->
	<link href="https://fonts.googleapis.com/css?family=Josefin+Sans:400,400i,600,600i,700" rel="stylesheet">

	<!-- Stylesheets -->
	<link rel="stylesheet" href="css1/bootstrap.min.css"/>
	<link rel="stylesheet" href="css1/font-awesome.min.css"/>
	<link rel="stylesheet" href="css1/magnific-popup.css"/>
	<link rel="stylesheet" href="css1/style.css"/>


	<!--[if lt IE 9]>
	  <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
	  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
	<![endif]-->

</head>
<body>
	<!-- Page Preloder -->
	<div id="preloder">
		<div class="loader"></div>
	</div>

	<!-- header section start -->
	<header class="header-section">
		<div class="container">
			<div class="row">
				<div class="col-lg-4 col-md-3">
					<div class="logo">
						<h2 class="site-logo">Photo Bucket</h2>
					</div>
				</div>
				<div class="col-lg-8 col-md-9">
					<p class="site-btn header-btn">Hi <%= user %></p>
				</div>
			</div>
		</div>
		<div class="nav-switch">
			<i class="fa fa-bars"></i>
		</div>
	</header>
	<!-- header section end -->

	<!-- intro section start -->
	<section class="intro-section">
		<div class="container text-center">
			<div class="row">
				<div class="col-xl-10 offset-xl-1">
					<h2 class="section-title">Photo Bucket. <span>designed by photographer</span>, for photographers</h2>
				</div>
			</div>
		</div>
	</section>
	<!-- intro section end -->


	<!-- portfolio section start -->
	<section class="portfolio-section">
		<div class="container">
			<ul class="portfolio-filter controls">
				<li class="control" data-filter="all"></li>
			</ul>
		</div>                       
		<div class="container-fluid p-md-0 ">
			<div class="row portfolios-area">
				<div class="mix col-lg-6 col-md-6 web">
					<a href="img1/portfolio/1.jpg" class="portfolio-item set-bg" data-setbg="img1/portfolio/1.jpg">
						<div class="pi-inner">
							<h2>Preview image</h2>
						</div>						
					</a>
				</div>
				<div class="mix col-lg-6 col-md-6 digital">
					<a href="img1/portfolio/2.jpg" class="portfolio-item set-bg " data-setbg="img1/portfolio/2.jpg">
						<div class="pi-inner">
							<h2>Preview image</h2>
						</div>						
					</a>
				</div>
				<div class="mix col-lg-4 col-md-6 web">
					<a href="img1/portfolio/3.jpg" class="portfolio-item set-bg" data-setbg="img1/portfolio/3.jpg">
						<div class="pi-inner">
							<h2>Preview image</h2>
						</div>						
					</a>
				</div>
				<div class="mix col-lg-4 col-md-6 digital">
					<a href="img1/portfolio/4.jpg" class="portfolio-item set-bg" data-setbg="img1/portfolio/4.jpg">
						<div class="pi-inner">
							<h2>Preview image</h2>
						</div>						
					</a>
				</div>
				<div class="mix col-lg-4 col-md-6 rened">
					<a href="img1/portfolio/5.jpg" class="portfolio-item set-bg" data-setbg="img1/portfolio/5.jpg">
						<div class="pi-inner">
							<h2>Preview image</h2>
						</div>						
					</a>
				</div>
				<div class="mix col-lg-12 col-md-6 brand">
					<a href="img1/portfolio/6.jpg" class="portfolio-item set-bg" data-setbg="img1/portfolio/6.jpg">
						<div class="pi-inner">
							<h2>Preview image</h2>
						</div>						
					</a>
				</div>
				<div class="mix col-lg-6 col-md-6 rened">
					<a href="img1/portfolio/7.jpg" class="portfolio-item set-bg" data-setbg="img1/portfolio/7.jpg">
						<div class="pi-inner">
							<h2>Preview image</h2>
						</div>						
					</a>
				</div>
				<div class="mix col-lg-6 col-md-6 brand">
					<a href="img1/portfolio/8.jpg" class="portfolio-item set-bg" data-setbg="img1/portfolio/8.jpg">
						<div class="pi-inner">
							<h2>Preview image</h2>
						</div>						
					</a>
				</div>
			</div>
		</div>
	</section>
	<!-- portfolio section end -->


	<!-- footer section start -->
	<footer class="footer-section text-center">
		<div class="container">
			<% if(user=="surya" || user=="raghav"){ %> 
					<h2 class="section-title mb-5">Got something new?</h2>
						<a href="login" class="site-btn" >Upload</a> 
			<% } else { %>
		  <% } %>
            <br><br><br> 
			<div class="copyright">
Copyright &copy;<script>document.write(new Date().getFullYear());</script> All rights reserved

</div>
		</div>
	</footer>



	<!--====== Javascripts & Jquery ======-->
	<script src="js1/jquery-2.1.4.min.js"></script>
	<script src="js1/bootstrap.min.js"></script>
	<script src="js1/mixitup.min.js"></script>
	<script src="js1/magnific-popup.min.js"></script>
	<script src="js1/main.js"></script>
	
</body>
</html>